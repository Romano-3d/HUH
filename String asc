    Scanner in = new Scanner(System.in);
    
        System.out.println("enter the size of the array : ");
        
    int size = in.nextInt();
    
        System.out.println("enter the elements : ");
        
    String[] arr = new String[size];
    
        for (int i = 0; i < arr.length; i++) {
            arr[i] = in.next();
            
        }
        
    Arrays.sort(arr);
         
    for (String str : arr) {
           
         System.out.println(str);
        
        }
